super + Return
	wezterm

super + space
	rofi -show run

# rofi
	# screenshot
	# screenrecord
	# power
	# passwords

super + Escape
	 pkill -USR1 -x sxhkd

super + q 
	bspc node -c 

super + shift + q 
	bspc node -k 

super + t
	bspc node -t tiled

super + o
	bspc node -t floating

super + f
	bspc node -t fullscreen

super + {h,j,k,l}
	bspc node -f {west,south,north,east}

super + shift + {h,j,k,l}
	bspc node -s {west,south,north,east}

super + {1-9,0}
	bspc desktop -f '^{1-9,10}'

super + shift +{1-9,0}
	bspc node -d '^{1-9,10}'

super + alt + {h,j,k,l}
	bspc node -p {west,south,north,east}

super + alt + {1-9}
	bspc node -o 0.{1-9}

super + alt + BackSpace
	bspc node -p cancel

super + alt + shift + BackSpace
	bspc query -N -d | xargs -I id -n 1 bspc node id -p cancel

super + ctrl + {h,j,k,l}
  bspc node -z {left -50 0 || bspc node -z right -50 0, \
                bottom 0 50 || bspc node -z top 0 50,\
                top 0 -50 || bspc node -z bottom 0 -50,\
                right 50 0 || bspc node -z left 50 0}
